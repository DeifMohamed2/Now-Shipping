      // Helper function to setup marker with drag listener
      function setupAddressMarker(map, marker, coordinatesInput, mapPointInput, geocoder, position) {
        if (marker) {
          marker.setPosition(position);
        } else {
          marker = new google.maps.Marker({
            position: position,
            map: map,
            draggable: true
          });
        }

        // Setup drag listener (remove old ones first to avoid duplicates)
        google.maps.event.clearListeners(marker, 'dragend');
        marker.addListener('dragend', function() {
          const pos = marker.getPosition();
          const coords = {
            lat: pos.lat(),
            lng: pos.lng()
          };
          coordinatesInput.value = JSON.stringify(coords);

          // Reverse geocode
          geocoder.geocode({ location: coords }, function(results, status) {
            if (status === 'OK' && mapPointInput) {
              mapPointInput.value = 'https://www.google.com/maps?q=' + coords.lat + ',' + coords.lng;
            }
          });
        });

        return marker;
      }

      // Helper function to update coordinates and reverse geocode
      function updateAddressLocation(coords, coordinatesInput, mapPointInput, geocoder, updateInput = true) {
        coordinatesInput.value = JSON.stringify(coords);
        
        if (updateInput && geocoder && mapPointInput) {
          geocoder.geocode({ location: coords }, function(results, status) {
            if (status === 'OK') {
              mapPointInput.value = 'https://www.google.com/maps?q=' + coords.lat + ',' + coords.lng;
            }
          });
        }
      }

      // Initialize map for a specific address card
      function initAddressMap(addressIndex, coordinates) {
        const addressCard = document.querySelector(`[data-address-index="${addressIndex}"]`);
        if (!addressCard) return;

        const mapContainer = addressCard.querySelector('.address-preview-map');
        const mapPointInput = addressCard.querySelector('.address-map-point');
        const coordinatesInput = addressCard.querySelector('.address-coordinates');
        const getCurrentLocationBtn = addressCard.querySelector('.get-current-location-btn');
        const openMapBtn = addressCard.querySelector('.open-map-btn');

        if (!mapContainer || !google || !google.maps) return;

        const defaultCoords = coordinates || { lat: 30.0444, lng: 31.2357 }; // Cairo default

        // Initialize map
        const map = new google.maps.Map(mapContainer, {
          center: defaultCoords,
          zoom: coordinates ? 15 : 10,
          mapTypeId: 'roadmap'
        });

        // Initialize geocoder
        const geocoder = new google.maps.Geocoder();

        let marker = null;
        if (coordinates && coordinates.lat && coordinates.lng) {
          marker = setupAddressMarker(map, marker, coordinatesInput, mapPointInput, geocoder, defaultCoords);
        }

        // Setup autocomplete for search
        if (mapPointInput) {
          const autocomplete = new google.maps.places.Autocomplete(mapPointInput);
          autocomplete.bindTo('bounds', map);

          autocomplete.addListener('place_changed', function() {
            const place = autocomplete.getPlace();
            if (!place.geometry) return;

            if (place.geometry.viewport) {
              map.fitBounds(place.geometry.viewport);
            } else {
              map.setCenter(place.geometry.location);
              map.setZoom(15);
            }

            const locationCoords = {
              lat: place.geometry.location.lat(),
              lng: place.geometry.location.lng()
            };
            
            marker = setupAddressMarker(map, marker, coordinatesInput, mapPointInput, geocoder, place.geometry.location);
            updateAddressLocation(locationCoords, coordinatesInput, mapPointInput, geocoder, false); // Don't update input, place already has address
            
            // Update stored map instance
            if (dashboardMaps[addressIndex]) {
              dashboardMaps[addressIndex].marker = marker;
            }
            
            // Save as Google Maps link
            mapPointInput.value = 'https://www.google.com/maps?q=' + locationCoords.lat + ',' + locationCoords.lng;
          });
        }

        // Add click listener to map
        map.addListener('click', function(event) {
          const coords = {
            lat: event.latLng.lat(),
            lng: event.latLng.lng()
          };

          marker = setupAddressMarker(map, marker, coordinatesInput, mapPointInput, geocoder, event.latLng);
          updateAddressLocation(coords, coordinatesInput, mapPointInput, geocoder);
          
          // Update stored map instance
          if (dashboardMaps[addressIndex]) {
            dashboardMaps[addressIndex].marker = marker;
          }
        });

        // Get Current Location Button Handler
        if (getCurrentLocationBtn) {
          // Remove existing listeners by cloning the button
          const newBtn = getCurrentLocationBtn.cloneNode(true);
          getCurrentLocationBtn.parentNode.replaceChild(newBtn, getCurrentLocationBtn);
          
          newBtn.addEventListener('click', function() {
            if (!navigator.geolocation) {
              alert('Geolocation is not supported by your browser');
              return;
            }

            // Show loading state
            const originalHTML = this.innerHTML;
            this.disabled = true;
            this.innerHTML = '<i class="ri-loader-2-line ri-spin me-1"></i> Getting...';

            navigator.geolocation.getCurrentPosition(
              function(position) {
                const coords = {
                  lat: position.coords.latitude,
                  lng: position.coords.longitude
                };

                // Center map on current location
                map.setCenter(coords);
                map.setZoom(15);

                // Update marker
                marker = setupAddressMarker(map, marker, coordinatesInput, mapPointInput, geocoder, coords);
                updateAddressLocation(coords, coordinatesInput, mapPointInput, geocoder);

                // Update stored map instance
                if (dashboardMaps[addressIndex]) {
                  dashboardMaps[addressIndex].marker = marker;
                }

                // Reset button
                newBtn.disabled = false;
                newBtn.innerHTML = originalHTML;
              },
              function(error) {
                console.error('Error getting location:', error);
                let errorMessage = 'Unable to retrieve your location';
                switch(error.code) {
                  case error.PERMISSION_DENIED:
                    errorMessage = 'Location access denied by user';
                    break;
                  case error.POSITION_UNAVAILABLE:
                    errorMessage = 'Location information unavailable';
                    break;
                  case error.TIMEOUT:
                    errorMessage = 'Location request timed out';
                    break;
                }
                alert(errorMessage);
                // Reset button
                newBtn.disabled = false;
                newBtn.innerHTML = originalHTML;
              },
              {
                enableHighAccuracy: true,
                timeout: 10000,
                maximumAge: 0
              }
            );
          });
        }

        // Store map instance
        dashboardMaps[addressIndex] = { map, marker, geocoder };
      }

