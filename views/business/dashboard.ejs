        // Handle remove button
        if (removeBtn) {
          removeBtn.addEventListener('click', function(e) {
            e.stopPropagation();
            if (pickupAddressCounter > 1) {
              actualCard.remove();
              pickupAddressCounter--;
              delete dashboardMaps[addressIndex];
              updateRemoveButtonsVisibility();
            }
          });
        }

        // Prevent collapse when clicking on form inputs, buttons, and selects
        if (actualCardBody) {
          const interactiveElements = actualCardBody.querySelectorAll('input, select, textarea, button, .select-area-btn');
          interactiveElements.forEach(element => {
            element.addEventListener('click', function(e) {
              e.stopPropagation();
            });
            element.addEventListener('mousedown', function(e) {
              e.stopPropagation();
            });
          });

          // Prevent collapse on map container but allow map interactions
          const mapContainer = actualCardBody.querySelector('.address-preview-map');
          if (mapContainer) {
            mapContainer.addEventListener('click', function(e) {
              e.stopPropagation();
            });
            mapContainer.addEventListener('mousedown', function(e) {
              e.stopPropagation();
            });
          }
        }

        // Initialize Bootstrap collapse after appending to DOM
        if (actualCard && typeof bootstrap !== 'undefined' && actualCollapseDiv && actualCardHeader) {
          // Initialize Bootstrap collapse
          const collapseInstance = new bootstrap.Collapse(actualCollapseDiv, {
            toggle: false
          });

          // Update icon rotation on collapse events
          actualCollapseDiv.addEventListener('show.bs.collapse', function() {
            actualCardHeader.setAttribute('aria-expanded', 'true');
            actualCardHeader.classList.remove('collapsed');
          });

          actualCollapseDiv.addEventListener('hide.bs.collapse', function() {
            actualCardHeader.setAttribute('aria-expanded', 'false');
            actualCardHeader.classList.add('collapsed');
          });

          // Handle header click to toggle collapse
          actualCardHeader.addEventListener('click', function(e) {
            // Don't toggle if clicking on remove button or its children
            if (e.target.closest('.remove-address-btn')) {
              return;
            }
            collapseInstance.toggle();
          });
        }

        // Update remove buttons visibility
        updateRemoveButtonsVisibility();

        // Initialize map if Google Maps is already loaded
        if (typeof google !== 'undefined' && google.maps) {
          setTimeout(() => {
            initAddressMap(addressIndex, null);
          }, 500);
        }
      }

      function updateRemoveButtonsVisibility() {
        const addresses = document.querySelectorAll('.pickup-address-card');
        addresses.forEach((card, index) => {
          const removeBtn = card.querySelector('.remove-address-btn');
          if (addresses.length > 1) {
            removeBtn.style.display = 'block';
          } else {
            removeBtn.style.display = 'none';
          }
        });
      }

      // Add address button click handler
      const addAddressBtn = document.getElementById('addPickupAddressBtn');
      if (addAddressBtn) {
        addAddressBtn.addEventListener('click', addPickupAddress);
      }

      // Initialize on page load if location tab is visible
      if (document.getElementById('v-pills-location')) {
        initializePickupAddresses();
      }

      // Initialize maps when Google Maps API loads
      if (typeof google !== 'undefined' && google.maps) {
        // Maps already loaded, initialize existing addresses
        setTimeout(() => {
          document.querySelectorAll('.pickup-address-card').forEach((card, index) => {
            const addressIndex = card.getAttribute('data-address-index');
            if (addressIndex !== null) {
              const coordsInput = card.querySelector('.address-coordinates');
              let coordinates = null;
              if (coordsInput && coordsInput.value) {
                try {
                  coordinates = JSON.parse(coordsInput.value);
                } catch (e) {
                  console.error('Error parsing coordinates:', e);
                }
              }
              initAddressMap(parseInt(addressIndex), coordinates);
            }
          });
        }, 500);
      } else {
        // Wait for Google Maps to load
        window.addEventListener('load', function() {
          if (typeof google !== 'undefined' && google.maps) {
            setTimeout(() => {
              document.querySelectorAll('.pickup-address-card').forEach((card) => {
                const addressIndex = card.getAttribute('data-address-index');
                if (addressIndex !== null) {
                  const coordsInput = card.querySelector('.address-coordinates');
                  let coordinates = null;
                  if (coordsInput && coordsInput.value) {
                    try {
                      coordinates = JSON.parse(coordsInput.value);
                    } catch (e) {
                      console.error('Error parsing coordinates:', e);
                    }
                  }
                  initAddressMap(parseInt(addressIndex), coordinates);
                }
              });
            }, 500);
          }
        });
      }

      // Remove old map modal confirmation handler - now using inline maps
      // The old locationModal is no longer needed for multiple addresses
    });
  </script>

